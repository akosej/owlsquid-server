#-- Path necessary files
path.AccessLog=./access.log
---------------------------------------------------------------------------
#--path where the logs will be saved
folder.salve_logs=./salva_logs
---------------------------------------------------------------------------
#-- Default quota for users 50 mb 1mb=1048576 Bytes
default.quota=52428800
---------------------------------------------------------------------------
#-- Exposed server interface to clients
interface.server=eth0
#-- Exposed squid port
squid.port=3128
#-- Exposed squid port ssl
squid.port=3129
#--Hours in which the system will restart the quota and rotate the logs
jobs.restart=08:00:00 10:00:00 11:00:00 11:30:00 12:00:00 13:00:00 14:00:00 15:00:00 16:00:00
---------------------------------------------------------------------------
#--Redis server Ipaddress
redis.ip=127.0.0.1
#--Redis server Password
redis.pass=test
#--Redis server Port
redis.port=6379